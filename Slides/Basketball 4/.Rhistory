Games2 = Games %>%
mutate(Season=rep(c(2014,2015,2016,2017),each=82*30)) %>%
group_by(Team,Season) %>%
summarize(Win.Per=mean(WINorLOSS=="W"),
Scored=mean(TeamPoints),
