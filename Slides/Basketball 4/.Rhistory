head(Games2)
pythag.func=function(data,par){
R=data$Scored/data$Allowed
y=data$Win.Per
resid=y-(R^(par[1]))/(R^(par[1])+1)
return(sum(resid^2))
}
result=optim(par=c(13),fn=pythag.func,data=Games2,method="BFGS")
print(result$par[1])
#Application to Replacement Players
alpha=result$par[1]
R=68.7/98.7
Win%=(R^(alpha))/(R^(alpha)+1)
